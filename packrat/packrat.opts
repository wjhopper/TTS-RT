auto.snapshot: TRUE
use.cache: FALSE
print.banner.on.startup: auto
vcs.ignore.lib: TRUE
vcs.ignore.src: FALSE
external.packages:
    dplyr
    tibble
    knitr
    BH
    R6
    assertthat
    bindr
    bindrcpp
    glue
    pkgconfig
    plogr
    rlang
    rmarkdown
    pander
local.repos: 
load.external.packages.on.startup: FALSE
ignored.packages: 
quiet.package.installation: TRUE
snapshot.recommended.packages: FALSE
snapshot.fields:
    Imports
    Depends
    LinkingTo
